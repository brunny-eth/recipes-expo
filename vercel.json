{
  "version": 2,
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node"
    }
  ],
  "devCommand": "npx ts-node --project server/tsconfig.json server/index.ts",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server/index.ts"
    }
  ],
  "functions": {
    "server/index.ts": {
      "maxDuration": 60
    }
  }
}