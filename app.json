{
  "expo": {
    "name": "Meez",
    "slug": "meez",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/meezrecipes_icon.png",
    "scheme": "meez",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/meez_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#fefcf7"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.meez.recipes",
      "usesAppleSignIn": true,
      "entitlements": {
        "com.apple.developer.applesignin": ["Default"],
        "com.apple.developer.associated-domains": ["applinks:meez.app"]
      },
      "splash": {
        "image": "./assets/images/meez_logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#fefcf7"
      },
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["meez"]
          }
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.meez.recipes",
      "splash": {
        "image": "./assets/images/meez_logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#fefcf7"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/meezrecipes_icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f14248c8-b5d7-42a2-b029-29fbed6bcd9f"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/f14248c8-b5d7-42a2-b029-29fbed6bcd9f"
    }
  }
}