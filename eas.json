{
  "cli": {
    "version": ">= 16.11.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "BUNDLE_ID": "com.meez.recipes",
        "EXPO_DEBUG": "1"
      },
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "BUNDLE_ID": "com.meez.recipes"
      },
      "ios": { "simulator": false }
    },
    "production": {
      "developmentClient": false,
      "distribution": "store",
      "channel": "production",
      "autoIncrement": true,
      "env": {
        "BUNDLE_ID": "com.meez.recipes",
        "EXPO_DEBUG": "1"
      },
      "ios": {
        "simulator": false
        }
    }
  },
  "submit": {
    "production": {}
  }
}