{
  "cli": {
    "version": ">= 16.11.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "BUNDLE_ID": "com.meez.recipes",
        "EXPO_DEBUG": "1",
        "EXPO_PUBLIC_ENABLE_PAYWALL": "true",
        "EXPO_PUBLIC_REVENUECAT_KEY": "appl_gSwOLBSPbTBxfJyeoLVdVSInSZd"
      },
      "ios": {
        "simulator": false
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "BUNDLE_ID": "com.meez.recipes",
        "EXPO_PUBLIC_ENABLE_PAYWALL": "true",
        "EXPO_PUBLIC_REVENUECAT_KEY":"appl_gSwOLBSPbTBxfJyeoLVdVSInSZd"
      },
      "ios": { "simulator": false }
    },
    "production": {
      "developmentClient": false,
      "distribution": "store",
      "channel": "production",
      "autoIncrement": true,
      "env": {
        "BUNDLE_ID": "com.meez.recipes",
        "EXPO_PUBLIC_ENV": "production",
        "EXPO_PUBLIC_ENABLE_PAYWALL": "false"
      },
      "ios": {
        "simulator": false,
        "image": "latest"
      }
    }
  },
  "submit": {
    "production": {}
  }
}